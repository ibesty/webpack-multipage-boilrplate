		"postcss-url": "8.0.0",
		"purify-css": "^1.2.5",
		"purifycss-webpack": "^0.7.0",
		"raw-loader": "^0.5.1",
		"style-loader": "0.23.1",
		"uglifyjs-webpack-plugin": "2.0.1",
		"url-loader": "^1.0.1",
		"webpack": "^4.8.3",
		"webpack-bundle-analyzer": "3.0.2",
		"webpack-cli": "3.1.2",
