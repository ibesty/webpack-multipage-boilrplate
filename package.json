{
	"name": "webpack-multipage-boilrplate",
	"version": "1.0.0",
	"description": "Webpack multipage boilrplate to build frontend template or backend template",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack-dev-server --inline --config build/webpack.dev.conf.js --project default",
		"build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
		"serve": "cross-env NODE_ENV=production webpack --config build/webpack.dev.build.conf.js --project default"
	},
	"author": "bestie",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"autoprefixer": "9.1.5",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.0.4",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-3": "^6.24.1",
		"clean-webpack-plugin": "^0.1.19",
		"copy-webpack-plugin": "^4.5.1",
		"cross-env": "^5.1.5",
		"css-loader": "1.0.0",
		"exports-loader": "^0.7.0",
		"expose-loader": "^0.7.5",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "2.0.0",
		"glob": "^7.1.2",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^4.0.0-beta.2",
		"html-withimg-loader": "^0.1.16",
		"less": "^3.8.1",
		"less-loader": "^4.1.0",
		"mini-css-extract-plugin": "^0.4.4",
		"optimize-css-assets-webpack-plugin": "5.0.1",
		"postcss-import": "12.0.0",
